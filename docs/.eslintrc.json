{
    "env": {
      "browser": true,
      "es2021": true
    },
    "plugins": [
      "react",
      "react-hooks"
    ],
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "extends": [
        "airbnb-base",
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "rules": {
      "no-restricted-syntax": ["off"],
      "no-unused-vars": ["error", { "varsIgnorePattern": "_ignore", "argsIgnorePattern": "^_$" }],
      "comma-dangle": ["error", "never"],
      "no-warning-comments": ["warn"]
    }
}
