{
  "name": "approval-bot",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "postinstall": "per-env",
    "postinstall:development": "husky install",
    "postinstall:staging": "",
    "postinstall:production": "",
    "clean": "rm -rf ./coverage",
    "eslint": "eslint ./lib",
    "lint": "git config --global --add safe.directory /workspace/git-workspace; commitlint -V --from=HEAD~1 && eslint ./lib && ansible-lint ./ansible-cics-sdv-pipeline && flake8 ./ansible-cics-sdv-pipeline && pylint ./ansible-cics-sdv-pipeline --fail-under 5 --fail-on W",
    "pretest": "npm run clean && npm run lint",
    "test": "jest --coverage",
    "pre-commit": "npm run lint",
    "unit-test": "jest --coverage",
    "start": "probot run ./lib/app.js"
  },
  "dependencies": {
    "probot": "13.2.0",
    "per-env": "1.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "18.4.3",
    "@commitlint/config-conventional": "18.4.3",
    "eslint": "8.55.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-jest": "27.6.0",
    "husky": "^8.0.3",
    "jest": "29.7.0"
  },
  "author": "",
  "license": "ISC",
  "jest": {
    "verbose": true,
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "lib/**/*.js",
      "!**/node_modules/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 83,
        "functions": 98,
        "lines": 89
      }
    },
    "coverageReporters": [
      "html",
      "text"
    ]
  }
}
